<h1>Search!</h1>

<div class="users-search">
  <label> User:
    <input type="text">
  </label>
  <ul class="users"></ul>

</div>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to user.username, user_url(user) %>
      <%= render "follows/form", user: user %>
    </li>
  <% end %>
</ul>
